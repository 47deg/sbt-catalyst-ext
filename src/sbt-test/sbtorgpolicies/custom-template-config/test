# checks that orgCreateFiles sbt task works as expected when user provides a custom template folder

> orgCreateFiles

$ exists target/scala-2.11/resource_managed/org-policies/templates

$ exists LICENSE

$ exists CONTRIBUTING.md

# check the custom LICENSE file

> check

> set orgGithubSettings := sbtorgpolicies.model.GitHubSettings("myorg","custom-template-config","My Organization",url("http://myorg.com"),"hello@myorg.com",sbtorgpolicies.model.ApacheLicense)

> orgCreateFiles

$ exists LICENSE

> checkApache

> set orgGithubSettings := sbtorgpolicies.model.GitHubSettings("myorg","custom-template-config","My Organization",url("http://myorg.com"),"hello@myorg.com",sbtorgpolicies.model.MITLicense)

> orgCreateFiles

$ exists LICENSE

> checkMIT